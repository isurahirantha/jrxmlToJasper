	<queryString>
		<![CDATA[select * from (
SELECT 
  CARDNUMBER, 
  TRANSACTIONAMOUNT,
  SETTLEMENTDATE,
  TRANSACTIONDATE,
  TRANSACTIONTYPE,
  TRANSACTIONDESCRIPTION,
  CARDCATEGORYCODE,
  CASHADVANCEFEE,
   eodid,crdr,
ACCOUNTNO

FROM

(SELECT CAC.CARDNUMBER AS CARDNUMBER, NVL(ET.TRANSACTIONAMOUNT,'')AS TRANSACTIONAMOUNT,
  NVL(ET.SETTLEMENTDATE,'')AS SETTLEMENTDATE,
  NVL(ET.TRANSACTIONDATE,'')AS TRANSACTIONDATE,
  NVL(ET.TRANSACTIONTYPE,'')AS TRANSACTIONTYPE,
  NVL(ET.TRANSACTIONDESCRIPTION,'')AS TRANSACTIONDESCRIPTION,
  (SELECT CA.CARDCATEGORYCODE  FROM CARD CA WHERE CA.CARDNUMBER = CAC.CARDNUMBER) AS CARDCATEGORYCODE,
  
   NVL((select FEEAMOUNT from EODCARDFEE where STATUS='EDON' and FEETYPE=$P{feeCashAdType}
   and CARDNUMBER = cac.cardnumber and TRANSACTIONID = et.TRANSACTIONID),0.00) as CASHADVANCEFEE,
   et.eodid AS eodid,et.crdr AS crdr,
CAC.ACCOUNTNO AS ACCOUNTNO
FROM CARDACCOUNTCUSTOMER CAC
FULL OUTER JOIN   EODTRANSACTION ET ON
ET.ACCOUNTNO = CAC.ACCOUNTNO and et.cardnumber=cac.cardnumber and
ET.SETTLEMENTDATE >to_date($P{endDate},'YY-MM-DD')
and ET.ADJUSTMENTSTATUS IN ('NO') and ET.STATUS IN('EDON','CQRT')
right join  CARD CA on
  cac.cardnumber=ca.cardnumber
  where
  CAC.ACCOUNTNO =$P{AccountNo})
  UNION ALL 
  (SELECT CAC.CARDNUMBER AS CARDNUMBER, NVL(ET.TRANSACTIONAMOUNT,'')AS TRANSACTIONAMOUNT,
  NVL(ET.SETTLEMENTDATE,'')AS SETTLEMENTDATE,
  NVL(ET.TRANSACTIONDATE,'')AS TRANSACTIONDATE,
  NVL(ET.TRANSACTIONTYPE,'')AS TRANSACTIONTYPE,
  NVL(ET.TRANSACTIONDESCRIPTION,'')AS TRANSACTIONDESCRIPTION,
  (SELECT CA.CARDCATEGORYCODE  FROM CARD CA WHERE CA.CARDNUMBER = CAC.CARDNUMBER) AS CARDCATEGORYCODE,
  
   NVL((select FEEAMOUNT from EODCARDFEE where STATUS='EDON' and FEETYPE=$P{feeCashAdType}
   and CARDNUMBER = cac.cardnumber and TRANSACTIONID = et.TRANSACTIONID),0.00) as CASHADVANCEFEE,
   et.eodid AS eodid,et.crdr AS crdr,
CAC.ACCOUNTNO AS ACCOUNTNO
FROM CARDACCOUNTCUSTOMER CAC
FULL OUTER JOIN   EODTRANSACTION ET ON
ET.ACCOUNTNO = CAC.ACCOUNTNO and et.cardnumber=cac.cardnumber and
ET.SETTLEMENTDATE >to_date($P{endDate},'YY-MM-DD')
and ET.ADJUSTMENTSTATUS IN ('TTC028') and ET.STATUS IN('EDON','CQRT') 
right join EODCARDFEE EF on ET.TRANSACTIONID=EF.TRANSACTIONID
right join  CARD CA on
  cac.cardnumber=ca.cardnumber
  where
  CAC.ACCOUNTNO =$P{AccountNo}
  AND EF.STATUS='EDON' AND EF.FEETYPE=$P{feeCashAdType})
  )  
  ORDER BY
CASE
  WHEN CARDCATEGORYCODE = 'M' OR CARDCATEGORYCODE = 'A' OR CARDCATEGORYCODE = 'CO'
  THEN 1
  WHEN CARDCATEGORYCODE = 'S' OR CARDCATEGORYCODE = 'AS' OR  CARDCATEGORYCODE = 'COS' OR CARDCATEGORYCODE = 'FS'
  THEN 2
  WHEN CARDCATEGORYCODE = 'E'
  THEN 3
  WHEN CARDCATEGORYCODE = 'C'
  THEN 4
  ELSE 5
END,
CARDNUMBER,SETTLEMENTDATE,TRANSACTIONDATE]]>
